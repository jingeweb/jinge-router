{
  "name": "jinge-router",
  "version": "4.0.0",
  "description": "typed router for jinge web framework",
  "main": "dist/index.js",
  "type": "module",
  "types": "src/index.ts",
  "scripts": {
    "lint-fix": "eslint '**/*.{js,mjs,ts}' --fix && prettier --log-level=warn --write . !*.yaml",
    "lint": "tsc --noEmit && eslint '**/*.{js,mjs,ts}' && prettier --check . !*.yaml",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jingeweb/jinge-router.git"
  },
  "keywords": [
    "jinge",
    "router",
    "mvvm router",
    "web router",
    "typed router"
  ],
  "author": "Yuhang Ge <abeyuhang@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jingeweb/jinge-router/issues"
  },
  "homepage": "https://github.com/jingeweb/jinge-router#readme",
  "dependencies": {
    "path-to-regexp": "^8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.1.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.3"
  }
}
