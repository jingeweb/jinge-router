{
  "name": "jinge-router",
  "version": "4.0.0-alpha.4",
  "description": "lightweight router for jinge web framework",
  "main": "dist/jinge-router.prod.js",
  "module": "dist/jinge-router.prod.js",
  "type": "module",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/jinge-router.prod.js"
    },
    "./dev": {
      "types": "./src/index.ts",
      "default": "./dist/jinge-router.dev.js"
    },
    "./source": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build-dev": "NODE_ENV=development vite build --mode development -c scripts/vite.config.ts",
    "build-prod": "vite build -c scripts/vite.config.ts",
    "build": "rm -rf dist && pnpm build-dev && pnpm build-prod",
    "lint-fix": "eslint '**/*.{js,mjs,ts}' --fix && prettier --log-level=warn --write . !*.yaml",
    "lint": "tsc --noEmit && eslint '**/*.{js,mjs,ts}' && prettier --check . !*.yaml",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jingeweb/jinge-router.git"
  },
  "keywords": [
    "jinge",
    "router",
    "mvvm router",
    "web router"
  ],
  "author": "Yuhang Ge <abeyuhang@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jingeweb/jinge-router/issues"
  },
  "homepage": "https://github.com/jingeweb/jinge-router#readme",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.4",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.3.1",
    "husky": "^9.1.6",
    "jinge": "4.0.0-alpha.6",
    "jinge-compiler": "4.0.0-alpha.8",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8"
  },
  "peerDependencies": {
    "jinge": "^4"
  }
}
